{
  "version": 2,
  "build": {
    "env": {
      "GA_TRACKING_ID": "@coronavirus-ga-tracking-id"
    }
  },
  "redirects": [
    {
      "source": "/en",
      "destination": "/?lang=en",
      "statusCode": 301
    },
    {
      "source": "/zh",
      "destination": "/?lang=zh",
      "statusCode": 301
    }
  ]
}
